local placeScripts = {
    [537413528] = {"https://raw.githubusercontent.com/noggik/build/refs/heads/main/build%20a%20boat"},
    [10704789056] = {
        "https://raw.githubusercontent.com/noggik/drive1.1/refs/heads/main/drive1.lua",
        "https://raw.githubusercontent.com/noggik/drive1.1/refs/heads/main/deive2.lua"
    }
}

local function loadScript(url)
    local success, response = pcall(function()
        return game:HttpGet(url, true)
    end)

    if success then
        loadstring(response)()
    else
        warn("โหลดสคริปต์ล้มเหลว: " .. url)
    end
end

local placeId = game.PlaceId
if placeScripts[placeId] then
    print("โหลดสคริปต์สำหรับ PlaceId:", placeId)
    
    for _, url in ipairs(placeScripts[placeId]) do
        loadScript(url)
    end
else
    print("ไม่มีสคริปต์ที่กำหนดสำหรับ PlaceId:", placeId)
end
